# Server Configuration
PORT=3000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/gohub
# For MongoDB Atlas (cloud):
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/gohub?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=30d

# Admin Authentication
ADMIN_KEY=your-admin-key-change-this-in-production

# CORS (Optional - comma-separated list of allowed origins)
# Leave empty or use * for development
ALLOWED_ORIGINS=http://localhost:8081,http://localhost:19006,https://gohub-alpha.vercel.app

# Logging
LOG_LEVEL=info
