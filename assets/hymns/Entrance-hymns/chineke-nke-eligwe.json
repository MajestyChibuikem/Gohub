{
  "title": "CHINEKE NKE ELIGWE",
  "sections": [
    {
      "type": "verse",
      "title": "Verse 1",
      "content": [
        "Chineke nke eligwe,",
        "Ị dị n'elu etu;",
        "Ị kwaghachi anyi n'ụfọdụ,",
        "Ị kpalie anyi n'otito."
      ]
    },
    {
      "type": "chorus",
      "title": "Chorus",
      "content": [
        "Ị kpalie anyi n'otito,",
        "Ị kpaa anyi n'ezinụlọ;",
        "Ị zighị anyi n'ogbu mmadu,",
        "Ị zaa anyi n'ụzọ dị mma."
      ]
    },
    {
      "type": "verse",
      "title": "Verse 2",
      "content": [
        "Anyi gbara ifufe,",
        "Kerubim na Serubim;",
        "Ndi mmadu niile dị n'elu,",
        "Ka anyi dibia Chineke."
      ]
    }
  ]
}