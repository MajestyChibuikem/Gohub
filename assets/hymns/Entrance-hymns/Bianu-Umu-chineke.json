{
  "title": "BIANU UMU CHINEKE",
  "sections": [
    {
      "type": "verse",
      "title": "Verse 1",
      "content": [
        "Bianu umu Chineke,",
        "Ụmụ afo dị ize;",
        "Chukwu na-abịa na anyi,",
        "Ka anyi maara ya n'obi."
      ]
    },
    {
      "type": "chorus",
      "title": "Chorus",
      "content": [
        "Ọ dị n'etiti anyi di,",
        "Ọ dị n'etiti anyi;",
        "Ọ bụrụ na anyi aga n'ụzọ ya,",
        "Ọ ga-esi anyi na ulo."
      ]
    },
    {
      "type": "verse",
      "title": "Verse 2",
      "content": [
        "Anyi bụ umu Chineke,",
        "Ọ dị n'etiti anyi;",
        "N'ime obi dị mma,",
        "Ọ bụrụ na anyi dịghị ezi."
      ]
    }
  ]
}