{
  "title": "NARA ONYINYE ANYI",
  "sections": [
    {
      "type": "intro",
      "content": [
        "Key F 2/4",
        "By Dorothy Iperesw",
        "Nara Onyinye anyi Chineke",
        "Mere anyi ebere",
        "Nurunu olu anyi",
        "Nar'aririo anyi."
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Ekene diri Gi, Oseburuwa Chineke",
        "Nk'uwa niile, Onye nzoputa"
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Obu n'uju afoma Gi k'anyi siri",
        "n'at'achicha Anyi n'enye Gi."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "N'ezie o bu iha ka mmadu siri n'ihe",
        "ala mitara wee meputa."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Man'anyi n'ario gi",
        "K'Imee k'oghol'anyi",
        "Achicha n'enye ndu",
        "Achicha nzoputa"
      ]
    }
  ]
}