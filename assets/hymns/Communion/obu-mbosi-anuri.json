{
  "title": "OBU MBOSI ANURI",
  "sections": [
    {
      "type": "intro",
      "content": [
        "Key D 3/4",
        "By Fr. Stan Anih",
        "O bu mbosi anuli",
        "Jesu Kristi nonyere anyi"
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Bianu sekpuere Ya",
        "Bianu nye Ya otito",
        "O si n'eluigwe bian'uwa",
        "Maka ihi ndi nke Ya"
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Ya nwa bu Okwu Chukwu,",
        "Ya nwa bu nwa Chukwu",
        "Okwu nke kasi okwu"
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Okwu nke n'enye ndu",
        "Okwu nke n'enye udo",
        "Jesus, Jesu, Jesu bu uzo",
        "Jesu bu eziokwu."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Ya nwa meriri onwu.",
        "Ya nwa meriri njo.",
        "O nwuru onwu n'elu obe",
        "Mak'ihi ndi nke Ya."
      ]
    }
  ]
}