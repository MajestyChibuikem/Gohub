{
  "title": "AMỊ NDI UYO UMEM",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Ndien Ami nyemenede nam enye ọdọk",
        "K'akpatre usen",
        "Bọ Idem ye Iyip Christ ke afo",
        "Eyen yene uwem"
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Ami ndi uyo uwem",
        "Ami ndi mmọñ uwem",
        "Enye emi editade"
      ]
    },
