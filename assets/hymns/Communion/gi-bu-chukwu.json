{
  "title": "GI BU CHUKWU",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Gi bu Chukwu Gi kere madu nile",
        "Gi bu ugwo ndu ha"
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Dinwenu gi bu ihe nke m",
        "Gi bu onye nzoputam",
        "Dinwenu gi bu mgbodo nke m",
        "Onye ka m ga atu egwu?"
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Ndi ajo mmadu n'abia",
        "ka ha mekpa m'ahu",
        "Dinwenu gi bu mgbodo nke m",
        "Gi ga akwatu ndi irom."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Otu ihe ka m n'iro gi",
        "Dinwenu m na Chukwu m",
        "Ka m biri n'ime ulo Gi",
        "ubochi niile nke ndum."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Aga m'ekwe n'eto Gi",
        "Chukwu nuru olu nkem",
        "Zitara m onyinye Gi",
        "Nke I g'eji gbaa m ume."
      ]
    },
    {
      "type": "verse 5",
      "content": [
        "Ajukwala m n'iwe Gi,",
        "So Gi bu Onye inye aka m",
        "Kuziere m uzo Gi, Dinwenu O",
        "Chukwu nara anyi"
      ]
    },
    {
      "type": "verse 6",
      "content": [
        "Otito diri Gi Nna, Otito diri Nwa",
        "Otito diri Muo Nso uwatuwa niile",
        "A-men."
      ]
    }
  ]
}