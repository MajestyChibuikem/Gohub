{
  "title": "TANTUM ERGO SACRAMENTUM",
  "sections": [
    {
      "type": "verse 1",
      "content": [
        "Tantum Ergo Sacramentum",
        "Veneremur cernui;",
        "Et antiquum documentum",
        "Novo cedat ritui;",
        "Praestet fides supplementum",
        "Sensuum defectui."
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Genitori, Genitoque",
        "Laus et jubilatio,",
        "Salus, honor, virtus quoque,",
        "Sit et benedictio;",
        "Procedenti ab utroque",
        "Compar sit laudatio. Amen."
      ]
    }
  ]
}