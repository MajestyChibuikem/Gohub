{
  "title": "WELUNA ANYA NENE JESU",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Weluna anya, weluna anya ne-ele Jesu,",
        "N'ebe Okotu n'obe, n'ebe Okotu n'obe,",
        "O gbasara unu aka,",
        "K'Owere tiara unu."
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Unu bu ndi no n'elu uwa,",
        "N'ime afufu na mkpa,",
        "Mgbe nchekwube ji benata,",
        "Ekpo obi nwute unu."
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Mgbe ndi b'unu ji rapu uwa,",
        "Soso unu wenano,",
        "Jidesinu obi ke Iota,",
        "Jesu dili unu nso."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Mgbe mbosi onwu unu ji bia,",
        "Ikpe natu unu egwu,",
        "Jidenu richikwube n'enyi,",
        "Jesu no n'enu obe."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Bia kwá»¥tenu m unu nine,",
        "Ndi ife n'anyi si ike,",
        "Ndi ane bogbu azi,",
        "Unu ncha gezu ike."
      ]
    }
  ]
}