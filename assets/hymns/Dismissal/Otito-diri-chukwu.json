    {
      "type": "verse",
      "title": "Verse 2",
      "content": [
        "Ọ dị n'etiti anyi di,",
        "Ka anyi maara Ya n'obi."
      ]
    },
    {
      "type": "chorus",
      "title": "Chorus",
      "content": [
        "Ọ zaa anyi n'ụzọ dị mma,",
        "Maka ihunanya Ya."
      ]
