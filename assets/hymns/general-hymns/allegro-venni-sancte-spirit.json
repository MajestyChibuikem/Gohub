{
  "title": "Veni Sancte Spiritus (Come, Holy Spirit)",
  "sections": [
    {
      "type": "verse",
      "title": "Verse 1",
      "content": [
        "Veni, Sancte Spiritus,",
        "Et emitte caelitus",
        "Lucis tuae radium.",
        "Veni, pater pauperum,"
      ]
    },
    {
      "type": "verse",
      "title": "Verse 2",
      "content": [
        "Veni, lumen cordium.",
        "Deus consolator optime,",
        "Dulcis hospes animae,",
        "Dulce refrigerium."
      ]
    },
    {
      "type": "verse",
      "title": "Verse 3",
      "content": [
        "In labore requies,",
        "In aestu temperies,",
        "In fletu solatium.",
        "O lux beatissima,"
      ]
    },
    {
      "type": "verse",
      "title": "Verse 4",
      "content": [
        "Reple cordis intima",
        "Tuorum fidelium.",
        "Sine tuo numine",
        "Nihil est in homine,"
      ]
    },
    {
      "type": "verse",
      "title": "Verse 5",
      "content": [
        "Nihil est innoxium.",
        "Lava quod est sordidum,",
        "Riga quod est aridum,",
        "Sana quod est saucium."
      ]
    },
    {
      "type": "verse",
      "title": "Verse 6",
      "content": [
        "Flecte quod est rigidum,",
        "Fove quod est frigidum,",
        "Rege quod est devium.",
        "Da tuis fidelibus,"
      ]
    },
    {
      "type": "verse",
      "title": "Verse 7",
      "content": [
        "In te confidentibus,",
        "Sacrum septenarium,",
        "Da virtutis meritum,"
      ]
    },
    {
      "type": "verse",
      "title": "Verse 8",
      "content": [
        "Da salutis exitum,",
        "Da perpetuae gloriae",
        "Palmam sine fine. Amen."
      ]
    }
  ]
}
