{
  "title": "HOSANNA DIRI GI ONYE NWEM",
  "key": "G 4/4",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Hosanna diri Gi, Onye nwem,",
        "Hosanna Jerusalem, to be Dinwenu,",
        "Hosanna!"
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Obe Chukwu Nna nke eluigwe, Hosanna!",
        "N'ihi n'ogosi g'ebube Ya,",
        "Hosanna!"
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "O gbaputara ndi nke Ya, Hosanna!",
        "Aka Ike Ya k'O ji zuo ha,",
        "Hosanna!"
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "O ruru oru ebube Ya,",
        "Hosanna! Wee gosi",
        "Na so Ya bu Chukwu.",
        "Hosanna!"
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "O ziter'anyi, Messiah,",
        "Hosanna! Dinwenu anyi,",
        "Jesu Kristi.",
        "Hosanna!"
      ]
    },
    {
      "type": "verse 5",
      "content": [
        "Onye meriri ekwensu,",
        "Hosanna! Wee gbaputa anyi n'aka Ya.",
        "Hosanna!"
      ]
    }
  ]
}