{
  "title": "ONYE KERE UWA BIKO",
  "sections": [
    {
      "type": "refrain",
      "content": [
        "Onye kere uwa biko, nabata obi nwo dibo Gi,",
        "Biko Nna, ejila iwe zuru Ya n'ezi,",
        "Were ebere kwe k'obata,",
        "Na obodo oma ahu bu Jerusalem."
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Aku na ihe ekike,",
        "Ihe nile di n'uwa agafe go,",
        "Ụgbụa bụ zi oge Gi Dinwenu."
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Onye nwe anyi gbaghara,",
        "Onweghi onye puru izu oke,",
        "N'ihu Gi belu so ma o natara,",
        "N'aka Gi mgbaghara nke njo."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Uwa agafe go,",
        "Ihe nile di n'uwa agafe go,",
        "Enwerọzim onye inye aka ọzọ,",
        "Belu so Gi."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Ahu bụ ihe ahụhụ,",
        "Mụ onwe dị bu ndụ n'oge gara aga,",
        "Bụ ihe anụmanụ,",
        "Onye nwe m cheta mọ,",
        "Cheta mọ Nna enwerọzim ike ọzọ."
      ]
    }
  ]
}