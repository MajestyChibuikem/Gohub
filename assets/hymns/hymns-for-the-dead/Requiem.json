{
  "title": "REQUIEM",
  "sections": [
    {
      "type": "refrain",
      "content": [
        "Requiem aeternam dona eis, Domine,",
        "Et lux perpetua luceat eis."
      ]
    },
    {
      "type": "verse",
      "content": [
        "Requiem aeternam dona eis, Domine,",
        "Et lux perpetua luceat eis.",
        "Te decet hymnus, Deus, in Sion,",
        "Et tibi reddatur votum in Jerusalem.",
        "Exaudi orationem meam;",
        "Ad te omni caro veniet."
      ]
    }
  ]
}