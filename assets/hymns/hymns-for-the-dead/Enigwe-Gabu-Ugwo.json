{
  "title": "ENIGWE GABU UGWO",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Enigwe, (3x) gab' ugwo."
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Enigwe gab' ugwo",
        "Nke Chukwu gakwu anyi,",
        "K'anyi lo otito",
        "Di ebe Chukwu no."
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Enigwe gab' ugwo Ndi nine cholunu,",
        "Ony' obuna nacho, Genwe ik' ilu n'enu."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Enigwe gab' ugwo kwe n'okwu nke Chukwu",
        "Chekwiibe n'igelii. M'I deb' iwu Chukwu"
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Enigwe gab' ugwo Nwa oko i'nine,",
        "Jidide nchekwube! Oge di nwankenke."
      ]
    },
    {
      "type": "verse 5",
      "content": [
        "Enigwe gab' ugwo I cholu nkusili",
        "Mgbe iji no n'afufu; Wenite-anya n'enu."
      ]
    },
    {
      "type": "verse 6",
      "content": [
        "Enigwe gab' ugwo onwu meghel' uzo,",
        "Nkpul' obi je n'enu gabinyelu Chukwu."
      ]
    },
    {
      "type": "verse 7",
      "content": [
        "Enigwe gab' ugwo na ngoli n' otito,",
        "Anuli nke Enigwe ganadi ebebe."
      ]
    },
    {
      "type": "verse 8",
      "content": [
        "Enigwe gab' ugwo anya afunene",
        "Na nke nti nunulu ife bu-ifu Chukwu."
      ]
    }
  ]
}