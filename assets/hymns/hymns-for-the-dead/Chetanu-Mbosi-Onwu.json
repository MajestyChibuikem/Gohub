{
  "title": "CHETANU MBOSI ONWU",
  "sections": [
    {
      "type": "chorus",
      "content": [
        "Chetanu mbosi onwu,",
        "Nke gacilili madu,",
        "N'ikpe nke gakal' unu,",
        "Ugwo dili ndu unu."
      ]
    },
    {
      "type": "verse 1",
      "content": [
        "Amutal' anyi n'uwa,",
        "N'ime afufu na nkpa.",
        "Ndu anyi di nwankenke:",
        "Mbosi onemone."
      ]
    },
    {
      "type": "verse 2",
      "content": [
        "Anyi di ndu n'ututu,",
        "Nwul' anwu n'anyasi,",
        "Dika afifia n' im' ofia",
        "Madu ji nma mata."
      ]
    },
    {
      "type": "verse 3",
      "content": [
        "Unu bu ndi ome njo",
        "Ji ngala garube,",
        "Kwusinu n'iru ozu,",
        "Nedu ife unu gabu."
      ]
    },
    {
      "type": "verse 4",
      "content": [
        "Nnukwu madu n'ututu,",
        "Ozu n'ururu chi;",
        "Eze nke madu nine,",
        "If olili aruru."
      ]
    },
    {
      "type": "verse 5",
      "content": [
        "Unu garapu ego,",
        "Aku, n' if ekike;",
        "Ife nke gabal' unu",
        "Bu soso ezi-omume."
      ]
    },
    {
      "type": "verse 6",
      "content": [
        "Ife nke gadil' unu,",
        "Bu ikpe nke kwul' oto",
        "Enuigwe nke gabu ugwo,",
        "N'oge nke dili njo."
      ]
    }
  ]
}